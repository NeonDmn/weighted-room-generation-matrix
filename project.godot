; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="DORRoomGenerationMatrix"
config/description="An app to manage complicated interconnections between room spawns in my game Doors of Delirium"
config/version="1.0"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
run/max_fps=30
run/low_processor_mode=true
config/icon="res://icon.svg"

[autoload]

Global="*res://global.gd"
SignalLens="*res://addons/signal_lens/autoload/signal_lens_autoload.gd"
SignalBus="*res://signal_bus.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/signal_lens/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
